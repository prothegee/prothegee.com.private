{
    "custom_config": {
        "envDevel": false,
        "envStaging": true,
        "envRelease": false,
        "envDomain": "prothegee.com",
        "envProjectName": "prothegee-com-backendv1",
        "envProjectVersion": "v1.0.0",
        "envMongoDbMainConnection": "mongodb://localhost:27017",
        "envMongoDbMainDatabase": "prothegee_staging",
        "m_defaultSalt": "wrvbgueGmUeYw5q5bmgZSDRVxAs6es2WVnGsCSNFscWJfVcPKwUHhnd9SL3wbMaf",
        "m_kyP": 976573028,
        "m_ivP": 279203156
    },
    "ssl": {
        "cert": "../../prothegee.com.private/ssl/TBA/TBD.pem",
        "key": "../../prothegee.com.private/ssl/TBA/TBD.pem",
        "use_old_tls": false,
        "conf": [
            ["MinProtocol", "TLSv1.2"],
            ["MaxProtocol", "TLSv1.3"],
            ["-min_protocol ", "TLSv1.2"],
            ["-max_protocol ", "TLSv1.3"],
            ["Options", "-strict, -SessionTicket, Compression"],
            ["Curves", "P-256:P-384"],
            ["SignatureAlgorithms", "CDSA+SHA256:RSA+SHA256:DSA+SHA256"],
            ["-dhparam", "../../prothegee.com.private/ssl/prothegee_dhparam.pem"],
            ["DHParameters", "../../prothegee.com.private/ssl/prothegee_dhparam.pem"],
            ["-cipher", "ECDHE-ECDSA-AES128-GCM-SHA256:ECDHE-RSA-AES128-GCM-SHA256:ECDHE-ECDSA-AES256-GCM-SHA384:ECDHE-RSA-AES256-GCM-SHA384:ECDHE-ECDSA-CHACHA20-POLY1305:ECDHE-RSA-CHACHA20-POLY1305:DHE-RSA-AES128-GCM-SHA256:DHE-RSA-AES256-GCM-SHA384"],
            ["CipherString", "ECDHE-ECDSA-AES128-GCM-SHA256:ECDHE-RSA-AES128-GCM-SHA256:ECDHE-ECDSA-AES256-GCM-SHA384:ECDHE-RSA-AES256-GCM-SHA384:ECDHE-ECDSA-CHACHA20-POLY1305:ECDHE-RSA-CHACHA20-POLY1305:DHE-RSA-AES128-GCM-SHA256:DHE-RSA-AES256-GCM-SHA384"],
            ["ChainCAFile", "../../prothegee.com.private/ssl/TBA/TBD/AAACertificateServices.crt"],
            ["VerifyCAFile", "../../prothegee.com.private/ssl/TBA/TBD/AAACertificateServices.crt"],
            ["RequestCAFile", "../../prothegee.com.private/ssl/TBA/TBD/AAACertificateServices.crt"],
            ["-serverpref", "off"],
            ["ServerPreference", "off"],
            ["-no_ticket", "off"],
            ["-SessionTicket", "off"]
        ]
    },
    "listeners": [
        {
            "address": "0.0.0.0",
            "port": 80,
            "https": true,
            "cert": "../../prothegee.com.private/ssl/TBA/TBD.pem",
            "key": "../../prothegee.com.private/ssl/TBA/TBD.pem"
        },
        {
            "address": "0.0.0.0",
            "port": 443,
            "https": true,
            "cert": "../../prothegee.com.private/ssl/TBA/TBD.pem",
            "key": "../../prothegee.com.private/ssl/TBA/TBD.pem"
        }
    ],
    "mime": {
        "text/markdown": "md",
        "text/gemini": ["gmi", "gemini"],
        "text/css": ["css", "css.map"],
        "text/javascript": ["js", "mjs", "js.map", "framework.js", "loader.js", "data", "wasm"]
    },
